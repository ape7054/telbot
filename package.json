{
    "name": "mev-bot-price",
    "version": "1.0.0",
    "description": "Solana mev bot price",
    "main": "dist/mev.js",
    "scripts": {
        "grpc": "ts-node ./src/grpc.ts",
        "wallet": "ts-node ./src/wallet.ts",
        "tapchain": "ts-node./src/Tapchain.ts",
        "transaction": "ts-node ./src/transaction.ts",
        "build": "tsc",
        "pkg": "pkg --target node18-linux-x64 --output ./executable/mev-bot ./dist/mev.js",
        "mev": "ts-node ./src/mev.ts",
        "test": "jest"
    },
    "bin": "dist/mev.js",
    "devDependencies": {
        "@types/chai": "^5.2.1",
        "@types/jest": "^29.5.14",
        "@types/mocha": "^10.0.1",
        "chai": "^5.2.0",
        "javascript-obfuscator": "^4.1.1",
        "jest": "^29.7.0",
        "mocha": "^10.2.0",
        "ts-jest": "^29.3.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.2"
    },
    "pkg": {
        "assets": [
            "dist/**/*"
        ],
        "targets": [
            "node18-linux-x64"
        ],
        "outputPath": "executable"
    },
    "dependencies": {
        "@grammyjs/conversations": "^1.2.0",
        "@grammyjs/menu": "^1.2.2",
        "@grammyjs/router": "^2.0.0",
        "@grammyjs/runner": "^2.0.3",
        "@metaplex-foundation/js": "^0.20.1",
        "@orca-so/common-sdk": "^0.6.11",
        "@orca-so/whirlpools-client": "^1.0.4",
        "@orca-so/whirlpools-sdk": "^0.13.17",
        "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
        "@solana/spl-token": "^0.4.8",
        "@solana/web3.js": "^1.98.0",
        "@triton-one/yellowstone-grpc": "^0.5.0",
        "@types/node-telegram-bot-api": "^0.64.8",
        "axios": "^1.7.7",
        "bs58": "^6.0.0",
        "decimal.js": "^10.5.0",
        "esrun": "^3.2.26",
        "grammy": "^1.29.0",
        "ioredis": "^5.4.1",
        "jito-js-rpc": "^0.1.0",
        "node-telegram-bot-api": "^0.66.0",
        "redis": "^4.7.0",
        "transaction": "^0.0.2",
        "tsc": "^2.0.4",
        "ws": "^8.18.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.ts"
        ],
        "moduleFileExtensions": [
            "ts",
            "js"
        ]
    }
}
